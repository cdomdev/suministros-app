---
interface Props {
  marca: string | null;
}
const { marca } = Astro.props;
export const prerender = true;
---

<ul>
  <li>
    <input
      class="m-2 rounded-[3px] w-3 uppercase h-3 focus:ring-2"
      type="checkbox"
      name="marca"
      value={marca}
      id={marca}
    />
    <label for={marca}>{marca}</label>
  </li>
</ul>
