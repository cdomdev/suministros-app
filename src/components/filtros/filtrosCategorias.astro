---
interface Props {
  categoria: string | null;
}
const { categoria } = Astro.props;
export const prerender = true;
---

<ul>
  <li>
    <input
      class="m-2 rounded-[3px] w-3 h-3 focus:ring-2 uppercase"
      type="checkbox"
      name="categoria"
      value={categoria}
      id={categoria}
    />
    <label for={categoria}>{categoria}</label>
  </li>
</ul>
